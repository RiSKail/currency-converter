    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "update-electron-app": "^2.0.1"
  },
  "scripts": {
    "react-start": "react-app-rewired start",
    "react-build": "set APP_ENV=react && react-app-rewired build",
    "react-test": "react-app-rewired test",
    "react-predeploy": "npm run build",
    "react-deploy": "gh-pages -d build",
    "react-eject": "react-app-rewired eject",
    "electron-build": "set APP_ENV=electron && react-app-rewired build && electron-builder --dir",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "build": {
    "appId": "com.example.currency-converter",
    "files": [
      "**/*",
      "build/**/*"
    ],
    "directories": {
      "buildResources": "static"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.1.3",
    "@types/react": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "concurrently": "^6.0.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "eslint-config-standard": "^14.0.1",
    "eslint-config-standard-react": "^9.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.1.0",
    "jest-image-snapshot": "^4.4.0",
    "jest-styled-components": "^6.3.3",
    "jest-transform-css": "^2.0.0",
    "jest-transform-file": "^1.1.1",
    "jsdom-screenshot": "^3.2.0",
    "module-alias": "^2.2.2",
    "react-app-rewire-alias": "^1.0.1",
    "react-app-rewired": "^2.1.8",
    "react-hot-loader": "^4.12.12",
    "redux-mock-store": "^1.5.4",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "wait-on": "^5.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/**/*.test.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.css$": "jest-transform-css",
      "^.+\\.svg$": "jest-transform-file",
      "^(?!.*\\.(json|svg)$)": "jest-transform-file"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
